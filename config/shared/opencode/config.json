    "CLIProxy": {
      "npm": "@ai-sdk/google",
      "name": "AGY (Google)",
      "options": {
        "baseURL": "http://localhost:8317/v1beta",
        "apiKey": "none",
      },
      "models": {
        "gemini-3-pro-preview": {
          "name": "Gemini 3 Pro (AGY)",
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image"
            ],
            "output": [
              "text"
            ]
          }
        },
        "gemini-claude-sonnet-4-5-thinking": {
          "name": "Sonnet 4.5 Think (AGY)",
          "limit": {
            "context": 200000,
            "output": 64000
          }
        }
      }
    },
    "cursor-proxy": {
      "name": "Cursor Proxy",
      "npm": "@ai-sdk/openai-compatible",
      "models": {
        "claude-4.5-sonnet": {
          "name": "Claude 4.5 Sonnet",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "reasoning": true,
        }
      },
      "options": {
        "baseURL": "http://localhost:3010/v1"
      }
    }
