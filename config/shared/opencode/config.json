{
  "$schema": "https://opencode.ai/config.json",
  "plugin": ["@tarquinen/opencode-dcp@latest", "./plugin/aa"],
  "small_model": "antigravity/gemini-3-flash",
  "model": "antigravity/claude-opus-4.5",
  "provider": {
    "google": {
      "models": {
        "gemini-3-pro-high": {
          "name": "Gemini 3 Pro High (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65535
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"]
          }
        },
        "claude-opus-4-5-thinking": {
          "name": "Claude Opus 4.5 (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"]
          }
        }
      }
    },
    "Antigravity": {
      "name": "Antigravity",
      "npm": "@ai-sdk/openai-compatible",
      "models": {
        "antigravity/claude-sonnet-4.5": {
          "name": "Claude 4.5 Sonnet (Reasoning)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "variants": {
            "high": {
              "reasoningEffort": "high"
            },
            "medium": {
              "reasoningEffort": "medium"
            },
            "low": {
              "reasoningEffort": "low"
            }
          },
          "modalities": {
            "input": ["text", "image"],
            "output": ["text"]
          },
          "cost": {
            "input": 3,
            "output": 15,
            "cache_read": 0.3,
            "cache_write": 3.75
          }
        },
        "antigravity/claude-opus-4.5": {
          "name": "Claude 4.5 Opus (Reasoning)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "reasoning": true,
          "variants": {
            "xhigh": {
              "reasoningEffort": "high"
            },
            "medium": {
              "reasoningEffort": "medium"
            },
            "low": {
              "reasoningEffort": "minimal"
            }
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"]
          },
          "cost": {
            "input": 5,
            "output": 25,
            "cache_read": 0.5,
            "cache_write": 6.25
          }
        },
        "github_copilot/gpt-5.2-codex": {
          "name": "GPT 5.2 Codex (Copilot)",
          "limit": {
            "context": 272000,
            "output": 64000
          },
          "reasoning": true,
          "variants": {
            "test": {
              "reasoningEffort": "fasdfdsf"
            },
            "high": {
              "reasoningEffort": "xhigh"
            },
            "medium": {
              "reasoningEffort": "medium"
            },
            "low": {
              "reasoningEffort": "low"
            }
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"]
          },
          "cost": {
            "input": 5,
            "output": 25,
            "cache_read": 0.5,
            "cache_write": 6.25
          }
        },
        "antigravity/gemini-3-pro-preview": {
          "name": "Gemini 3 Pro (Reasoning)",
          "limit": {
            "context": 1000000,
            "output": 64000
          },
          "modalities": {
            "input": ["text", "image"],
            "output": ["text"]
          },
          "cost": {
            "input": 5,
            "output": 25,
            "cache_read": 0.5,
            "cache_write": 6.25
          }
        },
        "codex/gpt-5.2": {
          "name": "GPT-5.2 (Reasoning)",
          "limit": {
            "context": 272000,
            "output": 32000
          },
          "modalities": {
            "input": ["text", "image"],
            "output": ["text"]
          },
          "variants": {
            "xhigh": {
              "reasoningEffort": "xhigh"
            },
            "medium": {
              "reasoningEffort": "medium"
            },
            "low": {
              "reasoningEffort": "low"
            }
          },
          "reasoning": true
        },
        "codex/gpt-5.2-codex": {
          "name": "GPT-5.2 Codex (Reasoning)",
          "limit": {
            "context": 272000,
            "output": 32000
          },
          "modalities": {
            "input": ["text", "image"],
            "output": ["text"]
          },
          "variants": {
            "xhigh": {
              "reasoningEffort": "xhigh"
            },
            "medium": {
              "reasoningEffort": "medium"
            },
            "low": {
              "reasoningEffort": "low"
            }
          },
          "reasoning": true
        }
      },
      "options": {
        "baseURL": "$PROXY_BASE_URL",
        "apiKey": "$PROXY_API_KEY"
      }
    }
  },
  "mcp": {
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "null"
      },
      "enabled": true
    },
    "mobile-mcp": {
      "type": "local",
      "enabled": true,
      "command": ["bunx", "-y", "@mobilenext/mobile-mcp@latest"]
    },
    "playwright": {
      "type": "local",
      "command": ["bunx", "@playwright/mcp@latest"],
      "enabled": true
    },

    "tavily": {
      "type": "local",
      "enabled": true,
      "command": [
        "npx",
        "-y",
        "mcp-remote",
        "https://mcp.tavily.com/mcp/?tavilyApiKey=null"
      ]
    }
  }
}
