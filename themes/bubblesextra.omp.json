{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "base": "#191724",
    "surface": "#1f1d2e",
    "overlay": "#26233a",
    "muted": "#6e6a86",
    "subtle": "#908caa",
    "text": "#e0def4",
    "love": "#eb6f92",
    "gold": "#f6c177",
    "rose": "#ebbcba",
    "pine": "#31748f",
    "foam": "#9ccfd8",
    "iris": "#c4a7e7",
    "highlight-low": "#21202e",
    "highlight-med": "#403d52",
    "highlight-high": "#524f67",
    "true": "p:pine",
    "false": "p:love",
    "time": "p:muted",
    "os-fg": "p:base",
    "os-bg": "p:subtle",
    "root-fg": "p:base",
    "root-bg": "p:love",
    "path": "p:gold",
    "git": "p:iris",
    "docker": "p:foam",
    "go": "p:rose",
    "haskell": "p:rose",
    "julia": "p:rose",
    "lua": "p:rose",
    "python": "p:rose",
    "rust": "p:rose"
  },
  "transient_prompt": {
    "template": "{{ now | date \"15:04:05\" }} {{ if gt .Code 0 }}<p:false>❯</>{{ else }}<p:true>❯</>{{ end }} ",
    "foreground": "p:time"
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "windows": "\ue70f"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ .Icon }} ",
          "foreground": "p:os-fg",
          "background": "p:os-bg",
          "type": "os",
          "style": "diamond",
          "foreground_templates": [
            "{{ if .Root }}p:root-fg{{ end }}"
          ],
          "background_templates": [
            "{{ if .Root }}p:root-bg{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " {{ .Path }} ",
          "foreground": "p:base",
          "background": "p:path",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_stash_count": false,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "p:base",
          "background": "p:git",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:love{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:foam{{ end }}",
            "{{ if gt .Ahead 0 }}p:pine{{ end }}",
            "{{ if gt .Behind 0 }}p:rose{{ end }}"
          ],
          "type": "git",
          "style": "diamond"
        },
        {
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <p:love>\ue5fa</> ",
            "yarn_icon": " <p:foam>\ue6a7</>"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "p:base",
          "background": "p:pine",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:base",
          "background": "p:gold",
          "type": "java",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:base",
          "background": "p:foam",
          "type": "dotnet",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue627 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:base",
          "background": "p:go",
          "type": "go",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue61f {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:base",
          "background": "p:haskell",
          "type": "haskell",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:base",
          "background": "p:julia",
          "type": "julia",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue620 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:base",
          "background": "p:lua",
          "type": "lua",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:base",
          "background": "p:rust",
          "type": "rust",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \udb81\udd1f {{ .FormattedMs }} ",
          "foreground": "p:base",
          "background": "p:true",
          "type": "executiontime",
          "style": "diamond",
          "background_templates": [
            "{{ if .Code }}p:false{{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "always",
            "display_default": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue73c {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} ({{ .Major }}.{{ .Minor }}){{ else }}{{ .Major }}.{{ .Minor }}{{ end }}{{ end }} ",
          "foreground": "p:base",
          "background": "p:python",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:base",
          "background": "p:foam",
          "type": "dart",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:base",
          "background": "p:love",
          "type": "angular",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \uf791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:base",
          "background": "p:rose",
          "type": "ruby",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}} ",
          "foreground": "p:base",
          "background": "p:gold",
          "type": "aws",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " \uf308 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "foreground": "p:base",
          "background": "p:pine",
          "type": "kubectl",
          "style": "diamond"
        },
        {
          "properties": {
            "charged_icon": "\uf240 ",
            "charging_icon": "\uf1e6 ",
            "discharging_icon": "\ue234 "
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}%{{ end }}{{ .Error }} ",
          "foreground": "p:base",
          "background": "p:love",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}p:pine{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:gold{{end}}",
            "{{if eq \"Full\" .State.String}}p:foam{{end}}"
          ],
          "type": "battery",
          "style": "diamond"
        },
        {
          "properties": {
            "time_format": "_2,15:04"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " \uf073 {{ .CurrentDate | date .Format }} ",
          "foreground": "p:base",
          "background": "p:iris",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "❯ ",
          "foreground": "p:true",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:false{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
