{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "base": "#faf4ed",
    "docker": "p:foam",
    "false": "p:love",
    "foam": "#56949f",
    "git": "p:iris",
    "go": "p:rose",
    "gold": "#ea9d34",
    "haskell": "p:rose",
    "highlight-high": "#cecacd",
    "highlight-low": "#f4ede8",
    "highlight-med": "#dfdad9",
    "iris": "#907aa9",
    "julia": "p:rose",
    "love": "#b4637a",
    "lua": "p:rose",
    "muted": "#9893a5",
    "os-bg": "p:subtle",
    "os-fg": "p:base",
    "overlay": "#f2e9e1",
    "path": "p:gold",
    "pine": "#286983",
    "python": "p:rose",
    "root-bg": "p:gold",
    "root-fg": "p:base",
    "rose": "#d7827e",
    "rust": "p:rose",
    "subtle": "#797593",
    "surface": "#fffaf3",
    "text": "#575279",
    "time": "p:muted",
    "true": "p:pine"
  },
  "transient_prompt": {
    "template": "{{ now | date \"15:04:05\" }} {{ if gt .Code 0 }}<p:false>❯</>{{ else }}<p:true>❯</>{{ end }} ",
    "foreground": "p:time"
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "windows": "\ue70f"
          },
          "trailing_diamond": "\ue0b0",
          "template": " {{ .Icon }} ",
          "foreground": "p:os-fg",
          "background": "p:os-bg",
          "type": "os",
          "style": "diamond",
          "foreground_templates": [
            "{{ if .Root }}p:root-fg{{ end }}"
          ],
          "background_templates": [
            "{{ if .Root }}p:root-bg{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "p:path",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_stash_count": false,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "p:git",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf308 {{ .Context }} ",
          "foreground": "p:docker",
          "type": "docker",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue627 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:go",
          "type": "go",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue61f {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:haskell",
          "type": "haskell",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:julia",
          "type": "julia",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue620 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:lua",
          "type": "lua",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_default": false,
            "display_mode": "context"
          },
          "template": "\ue73c {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:python",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:rust",
          "type": "rust",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "template": "\udb81\udd1f {{ .FormattedMs }} ",
          "foreground": "p:true",
          "type": "executiontime",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Code }}p:false{{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "environment"
          },
          "template": "{{ .Venv }} ",
          "foreground": "p:python",
          "type": "python",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "❯ ",
          "foreground": "p:true",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:false{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
